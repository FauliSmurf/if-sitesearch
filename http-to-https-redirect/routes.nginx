server {
    listen 80;
    listen [::]:80;
    server_name sitesearch.cloud;

    #    return 301 https://$host$request_uri;

    location / {
        proxy_pass https://sitesearch.cloud;
    }

    location /dev {
        proxy_pass https://sitesearch.cloud/;
    }

    location /api {
        proxy_pass https://sitesearch.cloud;
    }

    location /about {
        proxy_pass http://sitesearch.online/;
    }

    #    return 301 https://$host$request_uri;
}

server {
    listen 80;
    listen [::]:80;
    server_name api.sitesearch.cloud;

    location / {
        proxy_pass https://sitesearch.online;
    }

    location /info {
        proxy_pass https://sitesearch.cloud;
    }
}

server {
    listen 80;
    listen [::]:80;
    server_name ci.sitesearch.cloud;

    location / {
        proxy_pass http://ci.sitesearch.cloud:8111;
    }
}

