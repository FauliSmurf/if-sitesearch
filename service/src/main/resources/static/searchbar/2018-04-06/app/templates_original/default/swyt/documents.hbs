<div class="ifs-media">
    <div class="ifs-media-left">
        <span data-action="preview" {{#if shouldUseCustomPreview}}data-preview-custom="{{customPreviewUrl}}"{{/if}} data-preview-id="{{urlbase64}}">
            <img src="{{rootDir}}/{{iconPath}}/{{fileExtension}}.png" onerror="this.src='{{rootDir}}/{{iconPath}}/_blank.png'">
        </span>
    </div>
    <div class="ifs-media-body">
        <h4 class="ifs-break-all">
            {{#if url}}
            <a href="{{url}}" target="_blank">{{filename}}</a>
            {{else}}
            <a href="javascript:void(0);" target="_blank">{{filename}}</a>
            {{/if}}
        </h4>
        {{#if modified}}
        <p>{{modified}}</p>
        {{/if}}
    </div>
</div>
