server {
    listen 80;
    listen [::]:80;

    include /etc/nginx/snippets/letsencrypt.conf;

    #    return 301 https://$host$request_uri;
    #

    location / {
        return 301 https://$host$request_uri;
        #            proxy_pass https://sitesearch.info/;
    }
}

server {
    #    listen 80;
    #    listen [::]:80;
    listen 443 ssl http2;
    listen [::]:443 ssl http2;
    server_name dev.sitesearch.cloud;

    include /etc/nginx/snippets/letsencrypt.conf;

    location / {
        proxy_pass https://sitesearch.info;
    }

    location /dev {
        proxy_pass https://sitesearch.cloud:2443/;
    }

    location /api {
        proxy_pass https://sitesearch.cloud:2443/;
    }

    location /about {
        proxy_pass http://sitesearch.online/;
    }
}

server {
    listen 80;
    listen [::]:80;
    server_name api.sitesearch.cloud;

    location / {
        proxy_pass http://sitesearch.online/;
    }

    location /api {
        proxy_pass https://api.sitesearch.cloud:2443;
    }

    location /info {
        proxy_pass https://sitesearch.cloud:2443;
    }
}

server {
    #    listen 80;
    #    listen [::]:80;
    listen 443 ssl http2;
    listen [::]:443 ssl http2;
    server_name ci.sitesearch.cloud;

    include /etc/nginx/snippets/letsencrypt.conf;

    location / {
        proxy_pass https://ci.sitesearch.cloud:8111;
    }
}

server {
    listen 80;
    listen [::]:80;
    server_name main.sitesearch.cloud;

    location / {
        proxy_pass http://sitesearch.online;
    }

    location /api {
        proxy_pass https://api.sitesearch.cloud:2443;
    }
}

server {
    listen 80;
    listen [::]:80;
    server_name dev.sitesearch.cloud;

    location /status {
        stub_status on;
        access_log off;
    }
}

server {
    listen 80;
    listen [::]:80;
    server_name test.sitesearch.cloud;

    location / {
        proxy_pass https://test.sitesearch.cloud:3442;
    }
}
